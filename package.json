{
  "name": "@todovue/tv-label",
  "private": false,
  "author": "Cristhian Daza",
  "description": "TvLabel is a lightweight and customizable label (chip) component designed to highlight statuses, categories, or tags within your UI.",
  "license": "MIT",
  "version": "1.2.0",
  "type": "module",
  "homepage": "https://ui.todovue.blog/label",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TODOvue/tv-label.git"
  },
  "bugs": {
    "url": "https://github.com/TODOvue/tv-label/issues"
  },
  "keywords": [
    "todovue",
    "front-end",
    "web",
    "vue",
    "vuejs",
    "vue-js",
    "label",
    "component",
    "vue-component",
    "custom-label"
  ],
  "exports": {
    ".": {
      "import": "./dist/tv-label.es.js",
      "require": "./dist/tv-label.cjs.js"
    },
    "./style.css": "./dist/tv-label.css",
    "./nuxt": "./nuxt.js"
  },
  "main": "dist/tv-label.cjs.js",
  "module": "dist/tv-label.es.js",
  "types": "dist/tv-label.d.ts",
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "nuxt.js"
  ],
  "sideEffects": [
    "*.css",
    "*.scss",
    "dist/*.css"
  ],
  "engines": {
    "node": ">=24.13.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:demo": "cp README.md public/ && cp CHANGELOG.md public/ && VITE_BUILD_TARGET=demo vite build"
  },
  "peerDependencies": {
    "vue": "^3.5.26"
  },
  "devDependencies": {
    "@todovue/tv-demo": "^1.4.0",
    "@vitejs/plugin-vue": "^6.0.3",
    "sass": "^1.97.2",
    "vite": "^7.3.1",
    "vite-plugin-dts": "^4.0.0"
  }
}
